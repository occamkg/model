FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

//internalField   uniform 5;
internalField   uniform 0; // 15; 190;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "oversetPatch"
    {
        type            overset;
        value           $internalField;
    }

    "(leftWing|rightWing|body)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    inlet
    {
        type  fixedValue;
        value $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    sides
    {
        type            zeroGradient;
    }

}

