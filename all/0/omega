FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 15;

boundaryField
{
    oversetPatch
    {
        type            overset;
        value           uniform 15;
    }
    sides
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 15;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 15;
        value           uniform 15;
    }
    leftWing
    {
        type            omegaWallFunction;
        value           uniform 15;
    }
    rightWing
    {
        type            omegaWallFunction;
        value           uniform 15;
    }
    body
    {
        type            omegaWallFunction;
        value           uniform 15;
    }
}


